---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<url>
		<loc>{{site.url}}</loc>
		<lastmod>2013-09-19</lastmod>
		<changefreq>weekly</changefreq>
		<priority>0.8</priority>
	</url>
    {% for post in site.posts %}
    {% if post.type != 'tweet' %}
	<url>
		<loc>{{site.url}}{{ post.url }}</loc>
		<lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
		<changefreq>never</changefreq>
		<priority>0.5</priority>
	</url>
    {% endif %}
    {% endfor %}
	<url>
		<loc>{{site.url}}/work</loc>
		<lastmod>2014-01-16</lastmod>
		<changefreq>never</changefreq>
		<priority>0.5</priority>
	</url>
	<url>
		<loc>{{site.url}}/photography</loc>
		<lastmod>2014-01-16</lastmod>
		<changefreq>yearly</changefreq>
		<priority>0.5</priority>
	</url>
	<url>
		<loc>{{site.url}}/search</loc>
		<lastmod>2014-02-20</lastmod>
		<changefreq>weekly</changefreq>
		<priority>0.2</priority>
	</url>
	<url>
		<loc>{{site.url}}/randomiser</loc>
		<lastmod>2013-01-19</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.8</priority>
	</url>
</urlset>